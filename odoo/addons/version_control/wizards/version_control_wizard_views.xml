<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_version_control_wizard" model="ir.ui.view">
            <field name="name">Version Control</field>
            <field name="model">version.control.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                    <footer>
                        <button string="确认" class="btn-primary" type="object" name="action_confirm"/>
                        <button string="取消" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_version_control_wizard" model="ir.actions.act_window">
            <field name="name">Version Control</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">version.control.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

    </data>
</odoo>    
